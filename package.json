{
  "name": "react-ssr-framework",
  "version": "1.0.0",
  "description": "A react ssr framework",
  "author": "lambert",
  "main": "index.js",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*": [
      "git add"
    ]
  },
  "scripts": {
    "dll": "webpack --config build/webpack.dll.config.js",
    "dev": "node server/index.js",
    "build:test": "rimraf .public && npm run build:client:test && npm run build:server:test",
    "build:client:test": "webpack --config build/webpack.client.test.config.js",
    "build:server:test": "webpack --config build/webpack.server.test.config.js",
    "start:test": "cross-env NODE_ENV=testing node server/index.js",
    "build": "rimraf .public && npm run build:client && npm run build:server",
    "build:client": "webpack --config build/webpack.client.prod.config.js",
    "build:server": "webpack --config build/webpack.server.prod.config.js",
    "start": "cross-env NODE_ENV=production node server/index.js",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\""
  },
  "dependencies": {
    "@loadable/component": "^5.10.1",
    "axios": "^0.18.0",
    "lodash": "^4.17.11",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.1",
    "react-redux": "^5.1.1",
    "react-router": "^5.1.2",
    "react-router-config": "^1.0.0-beta.4",
    "react-router-dom": "^4.4.0",
    "redux": "^4.0.1",
    "redux-actions": "^2.6.5",
    "redux-thunk": "^2.3.0",
    "toastr": "^2.1.4"
  },
  "devDependencies": {
    "@babel/core": "^7.8.3",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-decorators": "^7.4.4",
    "@babel/plugin-proposal-export-default-from": "^7.8.3",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.8.3",
    "@loadable/babel-plugin": "^5.12.0",
    "@loadable/server": "^5.9.0",
    "@loadable/webpack-plugin": "^5.7.1",
    "@types/node": "^13.1.6",
    "@types/react": "^16.9.17",
    "@types/react-dom": "^16.9.4",
    "@types/react-redux": "^7.1.5",
    "@types/react-router-dom": "^5.1.3",
    "@types/webpack-env": "^1.15.0",
    "@typescript-eslint/eslint-plugin": "^2.3.1",
    "@typescript-eslint/parser": "^2.3.1",
    "autoprefixer": "^9.5.1",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.6",
    "babel-plugin-dynamic-import-node": "^2.2.0",
    "babel-polyfill": "^6.26.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.4",
    "copy-webpack-plugin": "^5.0.3",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.1",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.13.0",
    "express": "^4.17.0",
    "express-http-proxy": "^1.5.1",
    "file-loader": "^2.0.0",
    "happypack": "^5.0.1",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^4.0.0-beta.3",
    "ip": "^1.1.5",
    "lint-staged": "^8.1.7",
    "mini-css-extract-plugin": "^0.4.5",
    "node-sass": "^4.13.1",
    "opn": "^6.0.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-flexbugs-fixes": "^4.1.0",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.19.1",
    "react-hot-loader": "^4.8.7",
    "rimraf": "^2.6.3",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "typescript": "^3.7.5",
    "url-loader": "^1.1.2",
    "webpack": "^4.31.0",
    "webpack-cli": "^3.3.2",
    "webpack-dev-middleware": "^3.7.0",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-merge": "^4.2.1",
    "webpack-node-externals": "^1.7.2"
  }
}
